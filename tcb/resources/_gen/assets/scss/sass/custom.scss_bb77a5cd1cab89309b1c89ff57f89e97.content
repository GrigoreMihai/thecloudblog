figure{text-align:center}figure>figcaption{font-style:normal;font-weight:500}.content-button{display:flex;align-items:center;font-weight:500;color:black}.lab-icon{border-bottom:1px dotted #00efd1;margin-right:5px;padding-bottom:2px;position:relative;display:inline-flex}.lab-icon:before{display:block;content:'';background-image:url(/lib/img/lab.svg);height:1.7rem;width:1.7rem}.lab-icon:hover:after{background:rgba(0,0,0,0.8);top:0px;font-weight:100;font-style:normal;font-size:15px;color:#fff;content:attr(data-title);left:100%;padding:0 5px 0;position:absolute;white-space:nowrap}.p-link--logo:before{background-image:url("/lib/img/logo.svg");background-size:60px 60px;background-repeat:no-repeat;background-position:center;display:block;width:60px;height:60px;content:"";margin:10px;float:left}dark-mode-toggle{--dark-mode-toggle-light-icon: url(/lib/img/moon.svg);--dark-mode-toggle-dark-icon: url(/lib/img/sun.svg);--dark-mode-toggle-icon-filter: invert(80%);--dark-mode-toggle-icon-size: 1.2rem}@keyframes blink{0%{opacity:0}20%{opacity:1}100%{opacity:0}}.btn-read-more span{margin-left:2px;font-size:20px;animation-name:blink !important;animation-duration:1.5s !important;animation-iteration-count:infinite !important;animation-fill-mode:both !important}.btn-read-more span:nth-child(2){animation-delay:.2s !important}.btn-read-more span:nth-child(3){animation-delay:.4s !important}

